{
  "name": "redux-act-reducer",
  "version": "1.5.1",
  "description": "A lib to create actions and reducers for Redux",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-core/register --require babel-polyfill test/*.js",
